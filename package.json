{
  "name": "@openwar/mongodb-queue",
  "version": "6.2.0",
  "type": "module",
  "description": "A lightweight MongoDB-based message queue with TypeScript support, atomic operations, and message deduplication",
  "main": "./dist/mongodb-queue.cjs",
  "module": "./dist/mongodb-queue.mjs",
  "types": "./dist/mongodb-queue.d.mts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/mongodb-queue.d.mts",
        "default": "./dist/mongodb-queue.mjs"
      },
      "require": {
        "types": "./dist/mongodb-queue.d.cts",
        "default": "./dist/mongodb-queue.cjs"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/openwar/mongodb-queue.git"
  },
  "author": "Filipe Guerra <alias.mac@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/openwar/mongodb-queue#readme",
  "keywords": [
    "mongodb",
    "mongo",
    "queue",
    "message-queue",
    "fifo",
    "typescript",
    "atomic",
    "deduplication",
    "bun"
  ],
  "files": [
    "dist"
  ],
  "scripts": {
    "lint": "concurrently 'npm:lint:*'",
    "lint:eslint": "eslint --max-warnings 0 .",
    "lint:ts": "tsc",
    "build": "tsdown",
    "prepare": "concurrently 'npm:prepare:*'",
    "prepare:build": "npm run build",
    "prepare:lefthook": "lefthook install",
    "test": "jest"
  },
  "peerDependencies": {
    "mongodb": "^5.7.0 || ^6.0.0 || ^7.0.0"
  },
  "peerDependenciesMeta": {
    "@types/mongodb": {
      "optional": true
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^20.3.0",
    "@commitlint/config-conventional": "^20.3.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^22.19.3",
    "commitlint": "^20.3.0",
    "concurrently": "^9.2.1",
    "conventional-changelog-cli": "^5.0.0",
    "eslint": "^9.39.2",
    "jest": "^30.0.0",
    "lefthook": "^2.0.0",
    "mongodb": "^5.9.2",
    "prettier": "^3.7.4",
    "standard-version": "^9.5.0",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "tsdown": "^0.19.0-beta.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.51.0"
  },
  "engines": {
    "bun": ">=1.2.20",
    "node": ">=20.19.0"
  },
  "standard-version": {
    "scripts": {
      "postchangelog": "prettier --write CHANGELOG.md"
    }
  }
}
