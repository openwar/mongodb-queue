{
  "name": "@openwar/mongodb-queue",
  "version": "6.2.0",
  "description": "A lightweight MongoDB-based message queue with TypeScript support, atomic operations, and message deduplication",
  "main": "dist/mongodb-queue.js",
  "module": "dist/mongodb-queue.es.js",
  "jsnext:main": "dist/mongodb-queue.es.js",
  "types": "dist/types/mongodb-queue.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/openwar/mongodb-queue.git"
  },
  "author": "Filipe Guerra <alias.mac@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/openwar/mongodb-queue#readme",
  "keywords": [
    "mongodb",
    "mongo",
    "queue",
    "message-queue",
    "fifo",
    "typescript",
    "atomic",
    "deduplication",
    "bun"
  ],
  "files": [
    "dist"
  ],
  "scripts": {
    "lint": "concurrently 'npm:lint:*'",
    "lint:eslint": "eslint .",
    "lint:ts": "tsc",
    "build": "concurrently 'npm:build:*'",
    "build:types": "tsc -p tsconfig.production.json",
    "build:js": "rollup --config rollup.config.ts --configPlugin typescript --configImportAttributesKey with",
    "prepare": "concurrently 'npm:prepare:*'",
    "prepare:build": "npm run build",
    "prepare:husky": "husky install",
    "test": "jest"
  },
  "peerDependencies": {
    "mongodb": "^5.7.0 || ^6.0.0 || ^7.0.0"
  },
  "peerDependenciesMeta": {
    "@types/mongodb": {
      "optional": true
    }
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/preset-env": "^7.28.5",
    "@babel/preset-typescript": "^7.28.5",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@rollup/plugin-babel": "^6.1.0",
    "@rollup/plugin-commonjs": "^28.0.9",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-typescript": "^12.3.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^25.0.3",
    "builtin-modules": "^4.0.0",
    "commitlint": "^19.8.1",
    "concurrently": "^9.2.1",
    "conventional-changelog-cli": "^5.0.0",
    "eslint": "^9.39.2",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "lint-staged": "^15.5.2",
    "mongodb": "^5.9.2",
    "prettier": "^3.7.4",
    "rollup": "^4.54.0",
    "standard-version": "^9.5.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.51.0"
  },
  "engines": {
    "bun": ">=1.2.20",
    "node": ">=20.19.0"
  },
  "standard-version": {
    "scripts": {
      "postchangelog": "prettier --write CHANGELOG.md"
    }
  }
}
